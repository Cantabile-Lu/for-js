#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/Learn/for-js/node_modules/.pnpm/typescript@4.8.4/node_modules/typescript/bin/node_modules:/mnt/e/Learn/for-js/node_modules/.pnpm/typescript@4.8.4/node_modules/typescript/node_modules:/mnt/e/Learn/for-js/node_modules/.pnpm/typescript@4.8.4/node_modules:/mnt/e/Learn/for-js/node_modules/.pnpm/node_modules:/mnt/e/Learn/for-js/node_modules:/mnt/e/Learn/node_modules:/mnt/e/node_modules:/mnt/e/Learn/for-js/node_modules/.pnpm/cosmiconfig-typescript-loader@4.1.1_33d7b64ae90e9bb80d48469351c457b9/node_modules/cosmiconfig-typescript-loader/node_modules:/mnt/e/Learn/for-js/node_modules/.pnpm/cosmiconfig-typescript-loader@4.1.1_33d7b64ae90e9bb80d48469351c457b9/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/e/Learn/for-js/node_modules/.pnpm/typescript@4.8.4/node_modules/typescript/bin/node_modules:/mnt/e/Learn/for-js/node_modules/.pnpm/typescript@4.8.4/node_modules/typescript/node_modules:/mnt/e/Learn/for-js/node_modules/.pnpm/typescript@4.8.4/node_modules:/mnt/e/Learn/for-js/node_modules/.pnpm/node_modules:/mnt/e/Learn/for-js/node_modules:/mnt/e/Learn/node_modules:/mnt/e/node_modules:/mnt/e/Learn/for-js/node_modules/.pnpm/cosmiconfig-typescript-loader@4.1.1_33d7b64ae90e9bb80d48469351c457b9/node_modules/cosmiconfig-typescript-loader/node_modules:/mnt/e/Learn/for-js/node_modules/.pnpm/cosmiconfig-typescript-loader@4.1.1_33d7b64ae90e9bb80d48469351c457b9/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../typescript@4.8.4/node_modules/typescript/bin/tsserver" "$@"
else
  exec node  "$basedir/../../../../../typescript@4.8.4/node_modules/typescript/bin/tsserver" "$@"
fi
