#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/Learn/for-js/node_modules/.pnpm/typescript@4.8.4/node_modules/typescript/bin/node_modules:/mnt/e/Learn/for-js/node_modules/.pnpm/typescript@4.8.4/node_modules/typescript/node_modules:/mnt/e/Learn/for-js/node_modules/.pnpm/typescript@4.8.4/node_modules:/mnt/e/Learn/for-js/node_modules/.pnpm/node_modules:/mnt/e/Learn/for-js/node_modules:/mnt/e/Learn/node_modules:/mnt/e/node_modules:/mnt/e/Learn/for-js/node_modules/.pnpm/ts-node@10.9.1_2abc08acbb4fe741ae8218df9aa74f5c/node_modules/ts-node/node_modules:/mnt/e/Learn/for-js/node_modules/.pnpm/ts-node@10.9.1_2abc08acbb4fe741ae8218df9aa74f5c/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/e/Learn/for-js/node_modules/.pnpm/typescript@4.8.4/node_modules/typescript/bin/node_modules:/mnt/e/Learn/for-js/node_modules/.pnpm/typescript@4.8.4/node_modules/typescript/node_modules:/mnt/e/Learn/for-js/node_modules/.pnpm/typescript@4.8.4/node_modules:/mnt/e/Learn/for-js/node_modules/.pnpm/node_modules:/mnt/e/Learn/for-js/node_modules:/mnt/e/Learn/node_modules:/mnt/e/node_modules:/mnt/e/Learn/for-js/node_modules/.pnpm/ts-node@10.9.1_2abc08acbb4fe741ae8218df9aa74f5c/node_modules/ts-node/node_modules:/mnt/e/Learn/for-js/node_modules/.pnpm/ts-node@10.9.1_2abc08acbb4fe741ae8218df9aa74f5c/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../typescript@4.8.4/node_modules/typescript/bin/tsserver" "$@"
else
  exec node  "$basedir/../../../../../typescript@4.8.4/node_modules/typescript/bin/tsserver" "$@"
fi
