#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/Learn/for-js/node_modules/.pnpm/commitizen@4.2.5/node_modules/commitizen/bin/node_modules:/mnt/e/Learn/for-js/node_modules/.pnpm/commitizen@4.2.5/node_modules/commitizen/node_modules:/mnt/e/Learn/for-js/node_modules/.pnpm/commitizen@4.2.5/node_modules:/mnt/e/Learn/for-js/node_modules/.pnpm/node_modules:/mnt/e/Learn/for-js/node_modules:/mnt/e/Learn/node_modules:/mnt/e/node_modules:/mnt/e/Learn/for-js/node_modules/commitizen/bin/node_modules:/mnt/e/Learn/for-js/node_modules/commitizen/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/e/Learn/for-js/node_modules/.pnpm/commitizen@4.2.5/node_modules/commitizen/bin/node_modules:/mnt/e/Learn/for-js/node_modules/.pnpm/commitizen@4.2.5/node_modules/commitizen/node_modules:/mnt/e/Learn/for-js/node_modules/.pnpm/commitizen@4.2.5/node_modules:/mnt/e/Learn/for-js/node_modules/.pnpm/node_modules:/mnt/e/Learn/for-js/node_modules:/mnt/e/Learn/node_modules:/mnt/e/node_modules:/mnt/e/Learn/for-js/node_modules/commitizen/bin/node_modules:/mnt/e/Learn/for-js/node_modules/commitizen/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../commitizen/bin/git-cz" "$@"
else
  exec node  "$basedir/../commitizen/bin/git-cz" "$@"
fi
